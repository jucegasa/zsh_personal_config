# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
alias gtnewbranch="git checkout -b"
alias gtchk="git checkout"
alias gtaall="git add ."
alias gtcmm="git commit -m"
alias gtcamend="git commit --amend --no-edit"
alias gtpnewbranch="git push --set-upstream origin"
alias gtpf="git push -f"
alias gtpull="git fetch && git pull"
alias tardc="tar -xvf"
alias gdwbuild="./gradlew build"
alias gdwclean="./gradlew clean"
alias javarun="java -jar -Dnetworkaddress.cache.ttl=60 -Dnetworkaddress.cache.negative.ttl=10"
alias dkps="docker ps"
alias dkst="docker stats"
alias dkpsa="docker ps -a"
alias dkimgs="docker images"
alias dkcpup="docker-compose up -d"
alias dkcpdown="docker-compose down"
alias dkcpstart="docker-compose start"
alias dkcpstop="docker-compose stop"
alias dkrm="docker rm -f"
alias dkcplogs="docker-compose logs -f"
alias dkbuild="docker build --rm=true"
alias dklocalbuild="docker build --file=Dockerfile.local --rm=true"
alias dkrun="docker run"
alias s3ls="aws s3 ls --profile rappi-pay"
alias s3rm="aws s3 rm --profile rappi-pay"
alias s3mv="aws s3 mv --profile rappi-pay"
alias s3cp="aws s3 cp --profile rappi-pay"
alias awscredentials="open ~/.aws/credentials"
alias yo-pay="bash <(curl -s https://rpp-generator.dev.rappi.com/yo.sh)"
#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="/Users/jucegasa/.sdkman"
[[ -s "/Users/jucegasa/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/jucegasa/.sdkman/bin/sdkman-init.sh"
